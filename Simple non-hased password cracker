#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char password[] = "mypassword";
    char guess[11];
    int i, j, k, l;

    for (i = 'a'; i <= 'z'; i++) {
        guess[0] = i;
        for (j = 'a'; j <= 'z'; j++) {
            guess[1] = j;
            for (k = 'a'; k <= 'z'; k++) {
                guess[2] = k;
                for (l = 'a'; l <= 'z'; l++) {
                    guess[3] = l;
                    guess[4] = '\0';
                    if (strcmp(password, guess) == 0) {
                        printf("Password found: %s\n", guess);
                        return 0;
                    }
                }
            }
        }
    }

    printf("Password not found.\n");
    return 1;
}

























